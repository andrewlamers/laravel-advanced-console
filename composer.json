{
    "name": "andrewlamers/laravel-advanced-console",
    "description": "Advanced console interface",
    "type": "library",
    "keywords": [
        "laravel",
        "console"
    ],
    "authors": [
        {
            "name": "Andrew Lamers",
            "email": "andrew.lamers@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/console": "~7.0 | ~6.0 | ~5.4",
        "illuminate/database": "~7.0 | ~6.0 | ~5.4",
        "illuminate/events": "~7.0 | ~6.0 | ~5.4",
        "illuminate/config": "~7.0 | ~6.0 | ~5.4",
        "illuminate/support": "~7.0 | ~6.0 | ~5.4",
        "illuminate/mail": "~7.0 | ~6.0 | ^5.4",
        "symfony/process": "~5.0 | ~4.0 | ~3.0",
        "symfony/console": "~5.0 | ~4.0 | ~3.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Andrewlamers\\LaravelAdvancedConsole\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andrewlamers\\LaravelAdvancedConsole\\LaravelAdvancedConsoleServiceProvider"
            ]
        }
    }
}
